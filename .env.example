# API Configuration
# Set a secret API key to protect your endpoints
# Leave empty/commented to run without authentication (not recommended for production)
API_KEY=

# Web UI Configuration
# Set to 'false' to disable the web interface at /
ENABLE_WEB_UI=true

# Privacy & Logging
# Set to 'true' to log SMS content and phone numbers (NOT recommended for privacy)
# Defaults to 'false' to protect user privacy
ENABLE_SENSITIVE_LOGS=false

# Server Configuration
PORT=3000

# SSH Configuration
SSH_HOST=
SSH_USERNAME=
SSH_PASSWORD=
